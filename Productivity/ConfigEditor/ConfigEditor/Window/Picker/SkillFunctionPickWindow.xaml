<Window x:Class="ConfigEditor.SkillFunctionPickWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="请选择一个技能效果" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <ListBox x:Name="lsbFunctions" HorizontalAlignment="Left" Height="208" Margin="10,10,0,0" VerticalAlignment="Top" Width="272">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Label Content="{Binding Key}"></Label>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <Button Content="取消" HorizontalAlignment="Left" Margin="207,240,0,0" VerticalAlignment="Top" Width="75" Click="onBtn_Cancel"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="127,240,0,0" VerticalAlignment="Top" Width="75" Click="onBtn_Confirm"/>

    </Grid>
</Window>
